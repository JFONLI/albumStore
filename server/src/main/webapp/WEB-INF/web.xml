<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>AlbumServlet</servlet-name>
    <servlet-class>servlet.albumStoreServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AlbumServlet</servlet-name>
    <url-pattern>/albums/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ReviewServlet</servlet-name>
    <servlet-class>servlet.reviewServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ReviewServlet</servlet-name>
    <url-pattern>/review/*</url-pattern>
  </servlet-mapping>

</web-app>
